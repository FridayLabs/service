{
    "name": "fridaylabs/service",
    "description": "12 factor service skeleton",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "neronmoon",
            "email": "alistar.neron@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1",
        "vlucas/phpdotenv": "^2.4",
        "monolog/monolog": "^1.23",
        "slim/slim": "^3.0",
        "illuminate/support": "^5.4",
        "slim/php-view": "^2.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.2"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        },
        "files": [
            "app/helpers.php"
        ]
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ]
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
